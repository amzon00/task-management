{
  "name": "template-api",
  "version": "0.0.1",
  "description": "",
  "author": "Browserbite",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "check-branch-name": "chmod 755 ./.githooks/check_branch_name.sh && sh ./.githooks/check_branch_name.sh",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:start": "npm run db:update && npm run db:update:constants && node dist/main",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "pre-commit": "npm run check-branch-name && lint-staged",
    "seed:generate": "sequelize-cli seed:generate --name %npm_config_name%",
    "migration:generate": "sequelize-cli migration:generate --name %npm_config_name%",
    "db:seed": "sequelize-cli db:seed:all",
    "db:migrate": "sequelize-cli db:migrate",
    "db:update": "npm run db:migrate && npm run db:seed",
    "db:bets:seed:undo": "nsequelize-cli db:seed:undo:all",
    "db:operator:migrate:undo": "sequelize-cli db:migrate:undo:all",
    "db:revert": "npm run db:seed:undo && npm run db:migrate:undo ",
    "db:update:constants": "npm run db:seed:constants",
    "db:seed:constants": "ts-node -r tsconfig-paths/register src/seed.ts"
  },
  "lint-staged": {
    "*.ts": "eslint"
  },
  "dependencies": {
    "@google-cloud/storage": "^6.8.0",
    "@nestjs-modules/mailer": "^1.8.1",
    "@nestjs/axios": "^1.0.0",
    "@nestjs/bull": "^0.6.2",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/common": "^9.2.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.1",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/mapped-types": "1.2.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.1",
    "@nestjs/schedule": "^2.1.0",
    "@nestjs/sequelize": "^9.0.0",
    "@nestjs/swagger": "^6.1.4",
    "@nestjs/terminus": "^9.1.4",
    "@nestjs/throttler": "^3.1.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/express-handlebars": "^5.3.1",
    "bcrypt": "^5.1.0",
    "bull": "^4.10.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "express-basic-auth": "^1.2.1",
    "express-handlebars": "^5.3.5",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "joi": "^17.7.0",
    "moment-timezone": "^0.5.40",
    "mysql2": "^2.3.3",
    "nestjs-pino": "^3.1.1",
    "nodemailer": "^6.8.0",
    "otplib": "^12.0.1",
    "passport": "^0.6.0",
    "passport-custom": "^1.1.1",
    "passport-firebase-jwt": "^1.2.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "prettier": "^2.8.2",
    "qrcode": "^1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.0",
    "sequelize": "^6.28.0",
    "sequelize-typescript": "^2.1.5",
    "sharp": "^0.31.2",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@browserbite-io/eslint-config": "^1.0.3",
    "@browserbite-io/prettier-config": "^1.0.0",
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.1",
    "@types/bcrypt": "^5.0.0",
    "@types/bull": "^4.10.0",
    "@types/express": "^4.17.15",
    "@types/jest": "29.2.4",
    "@types/moment-timezone": "^0.5.30",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.11.17",
    "@types/passport-jwt": "^3.0.8",
    "@types/passport-local": "^1.0.34",
    "@types/qrcode": "^1.5.0",
    "@types/sequelize": "^4.28.14",
    "@types/sharp": "^0.31.0",
    "@types/supertest": "^2.0.12",
    "eslint-import-resolver-typescript": "^3.5.2",
    "husky": "^8.0.2",
    "jest": "^29.3.1",
    "lint-staged": "^13.1.0",
    "sequelize-cli": "^6.5.2",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.2",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "prettier": "@browserbite-io/prettier-config"
}
